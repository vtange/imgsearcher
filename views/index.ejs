<head>
    <meta charset="utf-8">
    <title>Barebones Page</title>
    <meta name="description" content="starter">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="style-0.css"/>
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.7/angular.min.js"></script>
    <script type="text/javascript" src="app.js"></script>
</head>
<body data-ng-app="imgsearcher" data-ng-controller="MainCtrl">
  <div class="content centered" data-ng-style="barHeight()">
	  <form data-ng-submit="imgSearch()">
	  	<input type="text" class="searchr" data-ng-model="userInputKeywords" placeholder="Search images">
	  </form>
	  <div class="gallery" data-ng-if="images"><div data-ng-repeat="image in images"><div class="rounded-white-box"><img data-ng-src="{{image.Thumbnail.MediaUrl}}"/></div></div></div>
      
      
  </div>
</body>
